{
  "name": "immunitet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:max-size": "node --max-old-space-size=8000 node_modules/nuxt/bin/nuxt.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build-and-start": "nuxt build && nuxt start",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,vue}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lintfix": "npm run lint:js -- --fix && npm run lint:style -- --fix",
    "lint-frontend": "lint-staged",
    "start-simpe-server": "node ./simple-server.js",
    "test": "jest --no-cache",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "cd ../.. && husky install .husky && husky add .husky/pre-commit \"npm run lint-frontend\""
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "^5.13.6",
    "axios": "^0.27.2",
    "core-js": "^3.19.3",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.12.3",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "webpack": "^4.46.0"
  }
}
